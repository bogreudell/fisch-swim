<div class="fs_product-thumbnail">
	<a href="{{ product.url }}">
		<div class="fs_product-thumbnail__image" style="background:url({{ product.featured_image | product_img_url: 'original' }}) center center no-repeat;"></div>
		<div class="fs_product-thumbnail__label">
			<p class="fs_product-thumbnail__label--title">{{ product.title }}</p>
			<p class="fs_product-thumbnail__label--price">{{ product.price | money }}</p>
		</div>
	</a>
</div>